      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarColor02">
      <ul class="navbar-nav me-auto">
        <li class="nav-item">
          <span class="visually-hidden">(current)</span>
            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">
                <img src="\images\logo.png" width="300" height="100" style="border-radius:5px; border:0px solid #bbb9b9" />
            </a>
            </li>
            <li class="nav-item">
                <span class="visually-hidden">(current)</span>
                <a class="nav-link " asp-area="Admin" asp-controller="Berlo" asp-action="Index">Bérlők</a>
            </li>
            <li class="nav-item">
                <span class="visually-hidden">(current)</span>
                <a class="nav-link " asp-area="" asp-controller="Home" asp-action="HaviNezet">Havi lekérdezés</a>
            </li>
            <li class="nav-item">
                <span class="visually-hidden">(current)</span>
                <a class="nav-link " asp-area="Admin" asp-controller="MeroOraTipus" asp-action="Index">Mérő óra típus</a>
            </li>
            <li class="nav-item">
                <span class="visually-hidden">(current)</span>
                <a class="nav-link " asp-area="Admin" asp-controller="Szerepkor" asp-action="Index">Szerepkör</a>
            </li>
            <li class="nav-item">
                <span class="visually-hidden">(current)</span>
                <a class="nav-link " asp-area="Admin" asp-controller="MeroOra" asp-action="Index">Mérőóra</a>
            </li>
      </ul>
      <partial name="_LoginPartial" />
      @*<form class="d-flex">
        <input class="form-control me-sm-2" type="text" placeholder="Search">
        <button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
      </form>*@
    </div>
  </div>
</nav>
    </header>





    <div class="container">

        <main role="main" class="pb-3">
            <partial name="_Notifaction" />
            @RenderBody()
        </main>
    </div>
    
    <footer class="footer text-muted" id="footer">
        
        <div class="container" >
            <hr>
            @*<img src="\images\dim.png" width:"100px" style="border-radius:5px; border:0px solid #bbb9b9"/>*@
